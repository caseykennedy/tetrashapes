{
  "name": "tetra-studio-website",
  "private": true,
  "description": "Tetra studio site + portfolio",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "bootstrap": "node ./scripts/bootstrap-fauna-database.js",
    "build": "gatsby build",
    "cleancache": "gatsby clean",
    "develop": "gatsby develop",
    "dev": "gatsby develop -o",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "ssr": "gatsby build && gatsby serve",
    "lint": "tslint -t stylish -p tsconfig.json",
    "lint:fix": "tslint -p tsconfig.json -t stylish --fix",
    "format": "prettier --write content/**/*.yaml",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@reach/dialog": "^0.10.5",
    "@reach/visually-hidden": "^0.10.4",
    "@react-three/drei": "^7.12.1",
    "@react-three/fiber": "^7.0.7",
    "@types/lodash": "^4.14.173",
    "@types/react-hamburger-menu": "0.0.3",
    "@types/react-typist": "^2.0.1",
    "@types/theme-ui": "^0.3.7",
    "@types/three": "^0.132.0",
    "babel-plugin-styled-components": "^1.12.0",
    "framer-motion": "^4.1.4",
    "gatsby": "^3.14.0",
    "gatsby-plugin-image": "^1.14.0",
    "gatsby-plugin-manifest": "^3.14.0",
    "gatsby-plugin-netlify": "^3.14.0",
    "gatsby-plugin-nprogress": "^3.14.0",
    "gatsby-plugin-offline": "^4.14.0",
    "gatsby-plugin-react-helmet": "^4.14.0",
    "gatsby-plugin-react-svg": "^3.0.1",
    "gatsby-plugin-sharp": "^3.14.0",
    "gatsby-plugin-sitemap": "^3.3.0",
    "gatsby-plugin-styled-components": "^4.14.0",
    "gatsby-plugin-theme-ui": "^0.11.2",
    "gatsby-plugin-typescript": "^3.14.0",
    "gatsby-source-filesystem": "^3.14.0",
    "gatsby-transformer-sharp": "^3.14.0",
    "lodash": "^4.17.20",
    "node-fetch": "^2.6.5",
    "polished": "^4.1.3",
    "react": "^17.0.2",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-dom": "^17.0.2",
    "react-fast-marquee": "^1.2.1",
    "react-hamburger-menu": "^1.2.1",
    "react-helmet": "^6.1.0",
    "react-is": "^16.13.1",
    "react-responsive-modal": "^6.0.1",
    "react-typist": "^2.0.5",
    "styled-components": "^5.3.1",
    "styled-normalize": "^8.0.7",
    "styled-system": "^5.1.5",
    "theme-ui": "^0.11.2",
    "three": "^0.132.2",
    "typescript": "^4.4.3"
  },
  "devDependencies": {
    "@types/node": "^14.17.18",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.3",
    "@types/react-helmet": "^6.1.1",
    "@types/styled-components": "^5.1.14",
    "@types/styled-system": "^5.1.13",
    "cross-env": "^5.2.1",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "start-server-and-test": "^1.14.0",
    "tslint": "^6.1.3",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "tslint-react": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/caseykennedy/tetra"
  },
  "bugs": {
    "url": "https://github.com/caseykennedy/tetra/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.yaml": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "tslint --fix",
      "git add"
    ],
    "*.{js,jsx}": [
      "xo --fix",
      "git add"
    ]
  }
}
