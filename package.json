{
  "name": "tetra-studio-website",
  "private": true,
  "description": "Tetra studio site + portfolio",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "bootstrap": "node ./scripts/bootstrap-fauna-database.js",
    "build": "gatsby build",
    "cleancache": "gatsby clean",
    "develop": "gatsby develop",
    "dev": "gatsby develop -o",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "ssr": "gatsby build && gatsby serve",
    "lint:js": "eslint --ext .ts,.tsx,.js,.jsx .",
    "lint:deadcode": "ts-prune -i '(test|gatsby)'",
    "lint:styles": "stylelint src/**/*.{js,jsx,ts,tsx,css}",
    "lint:format": "prettier --check '**/*.{js,jsx,ts,tsx,css}'",
    "lint:all": "npm run lint:js && npm run lint:styles && npm run lint:format && npm run lint:md",
    "format:js": "prettier '**/*.{js,jsx,ts,tsx}' --write",
    "format:styles": "prettier '**/*.scss' --write",
    "format:all": "npm run format:js && npm run format:styles",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@reach/dialog": "^0.10.5",
    "@reach/visually-hidden": "^0.10.4",
    "@react-three/fiber": "^7.0.24",
    "@sanity/block-content-to-react": "^2.0.7",
    "@types/lodash": "^4.14.168",
    "@types/react-hamburger-menu": "0.0.3",
    "@types/react-typist": "^2.0.1",
    "@types/theme-ui": "^0.6.0",
    "babel-plugin-styled-components": "^1.12.0",
    "eslint-config-airbnb": "^19.0.1",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-testing-library": "^5.0.1",
    "framer-motion": "^4.1.4",
    "gatsby": "^3.3.0",
    "gatsby-image": "^2.11.0",
    "gatsby-plugin-google-tagmanager": "^3.3.0",
    "gatsby-plugin-image": "^1.3.0",
    "gatsby-plugin-manifest": "^3.3.0",
    "gatsby-plugin-netlify": "^3.3.0",
    "gatsby-plugin-nprogress": "^3.3.0",
    "gatsby-plugin-offline": "^4.3.0",
    "gatsby-plugin-react-helmet": "^4.14.0",
    "gatsby-plugin-react-svg": "^3.0.1",
    "gatsby-plugin-sharp": "^3.3.0",
    "gatsby-plugin-sitemap": "^3.3.0",
    "gatsby-plugin-styled-components": "^4.3.0",
    "gatsby-plugin-theme-ui": "^0.12.1",
    "gatsby-plugin-typescript": "^3.3.0",
    "gatsby-source-filesystem": "^3.14.0",
    "gatsby-transformer-sharp": "^3.14.0",
    "gatsby-transformer-yaml": "^3.14.0",
    "lodash": "^4.17.20",
    "polished": "^4.1.1",
    "react": "^17.0.2",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-dom": "^17.0.2",
    "react-fast-marquee": "^1.2.1",
    "react-hamburger-menu": "^1.2.1",
    "react-helmet": "^6.1.0",
    "react-is": "^16.13.1",
    "react-responsive-modal": "^6.0.1",
    "react-toggle-dark-mode": "^1.0.4",
    "react-typist": "^2.0.5",
    "styled-components": "^5.2.1",
    "styled-normalize": "^8.0.7",
    "swiper": "^7.3.2",
    "theme-ui": "^0.7.5",
    "three": "^0.135.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.41",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-helmet": "^6.1.1",
    "@types/styled-components": "^5.1.9",
    "@types/three": "^0.135.0",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.3.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-webpack-plugin": "^3.1.1",
    "gatsby-plugin-eslint": "^4.0.2",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "start-server-and-test": "^1.12.0",
    "typescript": "^4.5.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/caseykennedy/bridge"
  },
  "bugs": {
    "url": "https://github.com/caseykennedy/bridge/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.yaml": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{js,jsx}": [
      "xo --fix",
      "git add"
    ]
  }
}
